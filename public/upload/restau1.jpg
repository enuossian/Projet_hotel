	window.fspanel_setActiveState = function(elements,active){
		if(active)
		{
			elements.addClass('active');
			elements.css({'pointer-events':'all'});
		}
		else
		{
			elements.removeClass('active');
			elements.css({'pointer-events':''});
		}
	}

	window.fspanel_closeAll = function(){

		window.fspanel_setActiveState($('.fsPanel'),false);


		$('.fspanel-active').removeClass('fspanel-active');
		$('body').removeClass("fspanel-active");
		$('body').removeAttr("fspanel");
		$('[fspanel-toggle]').removeClass('fspanel-active');
		$('[fspanel-open]').removeClass('fspanel-active');

		$('.fsPanel iframe').each(function(){
			var src = $(this).attr('src');
			var src_org = $(this).attr('src_org');
			if(!src_org)
				$(this).attr('src_org',src);

			//if(src.match(/(youtube|dailymotion|tube|vimeo|youtu)/))
			if(src_org && src_org!=='null')
				this.src = 'about:blank';
		})
	}

	window.fspanel_open_element = function(el,k)
	{
		window.fspanel_setActiveState(el,true);

		$('[fspanel-toggle="'+k+'"]').addClass('fspanel-active');
		$('[fspanel-open="'+k+'"]').addClass('fspanel-active');
		$('body').addClass("fspanel-active");
		$('body').attr("fspanel",k);

		el.find('iframe').each(function(){
			var src_org = $(this).attr('src_org');
			if(src_org && src_org!=='null')
			{
				this.src = src_org;
			}
		})
	}

	window.fspanel_close_burger = function()
	{
		if(typeof window.tophat_burger_close == 'function')
			window.tophat_burger_close();
	}

	window.fspanel_mark_as_ready = function(els)
	{
		$(els).filter('[data-status="loading"]').attr('data-status','init');
		setTimeout(function(els){
			$(els).filter('[data-status="init"]').attr('data-status','ready');
		}.bind(null,els),1000)
	}

	window.fspanel = function(k, action, lazy_action, ctx_type, ctx_code, iframe_url)
	{
		//console.log({ k, action, lazy_action, ctx_type, ctx_code, iframe_url });
		lazy_action = lazy_action || null;
		ctx_type = ctx_type || null;
		ctx_code = ctx_code || null;

		var action = action ? action:'open';
		if(action=='search')
		{
			if(ctx_type!==null)
				var el = $('#fspanel-'+k+'.fsPanel[data-ctx-type="'+ctx_type+'"][data-ctx-code="'+ctx_code+'"]');
			else
				var el = $('#fspanel-'+k+'.fsPanel[data-ctx-type=""]');
			if(el.length>0)
				return el;

			if(ctx_type!==null)
				var el = $('.fsPanel[data-code="'+k+'"][data-ctx-type="'+ctx_type+'"][data-ctx-code="'+ctx_code+'"]');
			else
				var el = $('.fsPanel[data-code="'+k+'"][data-ctx-type=""]');
			if(el.length>0)
				return el;

			var newPanel = $('<div class="fsPanel-lazy-item"></div>');
			$('.fsPanel-lazy-placeholder').append(newPanel);
			console.log('fsPanel : try to load panel with key = '+k+'#'+ctx_type+':'+ctx_code+') (current action is "'+action+'" and lazyaction will be "'+lazy_action+'")');
			newPanel.load(BASEPATH+ENV_LNG+'/ajax/fspanel?key='+k+'&ctx_type='+(ctx_type ? ctx_type:'')+'&ctx_code='+(ctx_code ? ctx_code:''),function(k,lazy_action,ctx_type,ctx_code,data){
				if(data.trim()!='error')
				{
					console.log('fsPanel : loading success');
					window.HAPI_RefreshJavascriptBehaviors();
					AjaxFormCommonListenerBuilder();
					window.fspanel_mark_as_ready(newPanel.find('.fsPanel'));
					setTimeout(window.fspanel.bind(null,k,lazy_action,null,ctx_type,ctx_code,iframe_url),150);
				}
				else
				{
					console.log('fsPanel : loading fail');
				}
			}.bind(null,k,lazy_action,ctx_type,ctx_code));

			return false;
		}
		else if(action=='toggle')
		{
			el = window.fspanel(k,'search','toggle',ctx_type,ctx_code,iframe_url);
			if(el && el.hasClass('active'))
			{
				// close
				window.fspanel_closeAll();
				window.fspanel_close_burger();
				return false;
			}
			else if(el)
			{
				if(iframe_url)
				{
					el.find('.fsPanel-iframe').attr('src','about:blank');
					setTimeout(function(el,src){
						el.find('.fsPanel-iframe').attr('src',iframe_url);
					}.bind(null,el,iframe_url))
				}

				// close all currently opened panel
				window.fspanel_closeAll();
				window.fspanel_close_burger();
				// then open the targeted one
				setTimeout(function(el,k){
					window.fspanel_open_element(el,k)
				}.bind(null,el,k),100)

				return true;
			}
			else
			{
				// try to load
				window.fspanel_closeAll();
				window.fspanel_close_burger();
				return false;
			}
		}
		else if(action=='open')
		{
			el = window.fspanel(k,'search','open',ctx_type,ctx_code);
			if(el)
			{
				if(iframe_url)
				{
					el.find('.fsPanel-iframe').attr('src','about:blank');
					setTimeout(function(el,src){
						el.find('.fsPanel-iframe').attr('src',iframe_url);
					}.bind(null,el,iframe_url))
				}

				// close all currently opened panel
				window.fspanel_closeAll();
				window.fspanel_close_burger();
				// then open the targeted one
				setTimeout(function(el,k){
					window.fspanel_open_element(el,k)
				}.bind(null,el,k),100)

				return true;
			}
		}
		else if(action=='close')
		{
			window.fspanel_closeAll();
			window.fspanel_close_burger();
			return false;
		}
	}
	</script>
	 			
				<script>
	// construction générique des écoutes d'évnèment sur les formulaires ajax
	// à utiliser n'importe ou pour gérer un formulaire qui serait injecté après le chargement initiale
	// dans un fspanel par exemple
	var AjaxFormCommonListenerBuilder = function()
	{
		if(false)
		{
			CaptchaOnloadCallback()
		}
		else
		{
			AjaxFormEventListenerInit();
		}
	}

	// basic event listener (recaptcha have a dedicated behavior)
	var AjaxFormEventListenerInit = function(form){
		var forms = [];
		if(form)
			forms = [form]
		else
			forms = document.querySelectorAll('form[data-ajax="true"]');

		for(var i=0; i<forms.length; i++)
		{
			if(forms[i].getAttribute('ajax-listener-ready')!='true')
			{
				forms[i].setAttribute('ajax-listener-ready','true')
				forms[i].addEventListener('submit', function(e)
				{
					e.preventDefault();
					e.stopPropagation();
					AjaxFormHandler(e.target)
					e.target.setAttribute('data-ajax','ready');
				});
			}
		}
	}
	var AjaxFormHandler = function(form)
	{
		form.classList.add('ajax-in-progress');
		if(typeof fetch ==='function')
		{
			fetch(form.action, {
				method: form.method,
				body: new FormData(form)
			}).then(function(result){
				if(result.status===200)
				{
					result.text().then(function(resultContent){
						AjaxFormSuccessCallback(form, resultContent );
					})
				}
			})
		}
		else
		{
			var request = new XMLHttpRequest();
			request.open(form.method, form.action);
			request.onload = function(oEvent) {
				if (request.status == 200) {
					AjaxFormSuccessCallback(form, request.response);
			  	}
			};
			request.send(new FormData(form));
		}
		form.classList.remove('ajax-in-progress');
	}
	var AjaxFormSuccessCallback = function(form,response)
	{
		form.outerHTML = response;
		window.HAPI_RefreshJavascriptBehaviors();
		AjaxFormCommonListenerBuilder();
	}
	</script>
