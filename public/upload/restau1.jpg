						}
					});
				}
			}

			function OverallClickCallback()
			{
				RealignBppContainer();
			}

			function DateClickCallback(type)
			{
				$el = $(window.LastBookingBppTriggerElement);
				if($el.data('on'+type)=='close')
				{
					BookingHideContainer();
				}
				else if($el.data('on'+type))
				{
					$elout = $($el.data('on'+type));
					if($elout.length>0)
					{
						//console.log($elout);
						BookingHideContainer(true);
						$elout.trigger('click.bpp');
					}
				}
			}

