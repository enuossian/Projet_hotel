			function BookingCallback(stayDatas)
			{
				BppHapiStays = stayDatas;

				//console.log('BookingCallback',stayDatas);
				$('.BookingInput[name=checkin]').val(stayDatas.checkin);
				$('.BookingInput[name=checkout]').val(stayDatas.checkout);
				$('.BookingInput[name=nights]').val(stayDatas.nights);
				$('.BookingInput[name=adults]').val(stayDatas.adults);
				$('.BookingInput[name=childs]').val(stayDatas.childs);
				$('.BookingInput[name=promo]').val(stayDatas.promo);
				$('.BookingInput[name=iata]').val(stayDatas.iata);

				if(stayDatas.price)
				{
					$('.BookingBestPrice').html(stayDatas.price.text)
					$('.BookingBestPrice-wrapper').fadeIn();
				}
				else
				{
					$('.BookingBestPrice').html('');
					$('.BookingBestPrice-wrapper').fadeOut();
				}
				BookingInputLabelUpdate();
			}

			function BookingInputLabelUpdate()
			{
				var date = $('.BookingInput[name=checkin]').val();
				if(date && date.match(/....-..-../) && $('.BookingInputCheckinLabel').length>0)
				{
					var d = date.split('-');
					//console.log(d);
					var event = new Date(Date.UTC(d[0], d[1]-1, d[2], 3, 0, 0));
					var f = $('.BookingInputCheckinLabel').data('format');
					if(f)
						$('.BookingInputCheckinLabel').val(dateFormat(event,f));
					else
						$('.BookingInputCheckinLabel').val(event.toLocaleDateString());
				}

				var date = $('.BookingInput[name=checkout]').val();
				if(date && date.match(/....-..-../) && $('.BookingInputCheckoutLabel').length>0)
				{
					var d = date.split('-');
					//console.log(d);
					var event = new Date(Date.UTC(d[0], d[1]-1, d[2], 3, 0, 0));
					var f = $('.BookingInputCheckoutLabel').data('format');
					if(f)
						$('.BookingInputCheckoutLabel').val(dateFormat(event,f));
					else
						$('.BookingInputCheckoutLabel').val(event.toLocaleDateString());
				}
			}

			// ---------------------------------------------------------------------------------
			// mise à jour du widget à partir des formulaires
			// ---------------------------------------------------------------------------------

			$('.BookingInput[name=HIDX]').on('change', function() {
				BookingInputLabelUpdate();
