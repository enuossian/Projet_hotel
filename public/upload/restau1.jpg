				{
					if($('.BookingInput[name=nights]').length>0)
						BppHapiComponent.app.setStay($('.BookingInput[name=checkin]').val(),null,$('.BookingInput[name=nights]').val());
					else if($('.BookingInput[name=checkout]').length>0)
						BppHapiComponent.app.setStay($('.BookingInput[name=checkin]').val(),$('.BookingInput[name=checkout]').val());
				}
			});
			$('.BookingInput[name=nights]').on('change', function() {
				BookingInputLabelUpdate();
